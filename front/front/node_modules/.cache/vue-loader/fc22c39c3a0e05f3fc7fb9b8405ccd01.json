reated() {\r\n    this.getPosts();\r\n    this.getTransfer();\r\n  },\r\n  computed: {\r\n    ...mapGetters(\"postsModule\", [\"posts\"]),\r\n    ...mapGetters(\"transferModule\", [\"transfers\"]),\r\n\r\n    slicePosts() {\r\n      return this.posts.slice(0, 4);\r\n    },\r\n\r\n    slicetransfers() {\r\n      return this.transfers.slice(0, 4);\r\n    },\r\n  },\r\n  mounted() {},\r\n};\r\n</script>\r\n\r\n"]}]}